{
  "name": "mintility",
  "version": "0.3.4",
  "description": "A small library of utility functions",
  "main": "index.js",
  "type": "module",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "run-s clean ts && rollup --config",
    "clean": "rimraf dist typings index.js",
    "lint": "run-s clean ts:silent && eslint src/**/*.ts --quiet --fix",
    "prepublish": "run-s build",
    "ts": "tsc -p .",
    "ts:silent": "tsc --noEmit -p .",
    "test": "jest"
  },
  "author": "Will Johnston <dev@wwj.dev>",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/jest": "^29.2.5",
    "@types/lodash": "^4.14.191",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "jest": "^29.3.1",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "rollup": "^3.9.1",
    "rollup-plugin-terser": "^6.1.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4"
  },
  "dependencies": {}
